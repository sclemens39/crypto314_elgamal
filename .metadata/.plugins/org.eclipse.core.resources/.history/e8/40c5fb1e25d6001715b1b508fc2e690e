package elgamal;
import java.util.*;
import java.math.*;

public class elgamal_ecryption {
	
	//p = large prime
	//a = primitive root of p
	//x = random int
	//b = a^x (mod p) 
	//k = key
	
	int p,a,b,k;
	
	int x = (int)Math.random()*100;
	
	int findPrim(int p){
		int prime = 0;
		
		if(isPrime(p)){
			// find primitive root of p
			
			return prime;
		}
		
		return prime;
	}
	
	boolean isPrime(int x){
		if(x%2 == 0){
			return true;
		}
		
		return false;
	}
}
